{
  "printWidth": 80,
  "tabWidth": 2,
  "trailingComma": "all",
  "singleQuote": true,
  "semi": true,
  "importOrder": [
    "^@[^/](.*)$",
    "^@/components/(.*)$",
    "^@/utils/(.*)$",
    "^@/styles/(.*)$",
    "^@/prisma/(.*)$",
    "^@/data/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
